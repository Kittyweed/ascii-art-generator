{
  "name": "corvin-ascii-gen",
  "version": "1.0.0",
  "description": "Image to ASCII art generator",
  "main": "dist/main.js",
  "homepage": "./",
  "type": "module",
  "scripts": {
    "dev": "concurrently \"npm run build:watch\" \"electron .\"",
    "build": "tsc",
    "build:watch": "tsc --watch",
    "start": "electron .",
    "pack": "electron-builder --dir",
    "dist": "electron-builder"
  },
  "dependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@types/react": "^18.2.37",
    "@types/react-dom": "^18.2.15",
    "electron": "^27.0.0",
    "typescript": "^4.9.5"
  }
}
